.search-wrap{
  width: 100%;
  height: 40px;
  border-bottom: 2px solid rgb(241, 241, 241);
  padding: 6px 10px;
}
.search-input{
  width: 500rpx;
  height: 35px;
  margin: 0 auto;
  padding-left: 60rpx;
  background: rgb(241, 241, 241);
  border-radius: 30rpx;
  font-size: 0.8rem;
  position: relative;
  float: left;
}
.search-input::after{
  content: "";
  position: absolute;
  left: 18rpx;
  top: 16rpx;
  width: 36rpx;
  height: 36rpx;
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
}
.search-btn{
  width: 60px;
  height: 35px;
  background-color: rgb(232, 60, 60);
  color: #ffffff;
  font-size: 11pt;
  line-height: 35px;
}